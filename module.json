{
	"id": "follow-the-leader",
	"title": "Follow the Leader",
	"description": "Set token(s) to follow another token, using routinglib to generate safe path.",
	"version": "1.0.1",
	"compatibility": {
		"minimum": "12",
		"verified": "12"
	},
	"esmodules": [
		"scripts/settings.js",
		"scripts/main.js"
	],
	"url": "https://github.com/thejoester/follow-the-leader/",
	"manifest": "https://github.com/thejoester/follow-the-leader/releases/latest/download/module.json",
	"download": "https://github.com/thejoester/follow-the-leader/releases/download/1.0.1/follow-the-leader.zip",
	"readme": "https://github.com/thejoester/follow-the-leader/blob/master/README.md",
	"changelog": "https://github.com/thejoester/follow-the-leader/blob/master/CHANGELOG.md",
	"license": "https://github.com/thejoester/follow-the-leader/blob/master/LICENSE.txt",
	"bugs": "https://github.com/thejoester/follow-the-leader/issues",
	"flags": {
		"allowBugReporter": true
	},
	"authors": [
		{
		  "name": "The Joester",
		  "discord": "thejoester",
		  "url": "https://github.com/thejoester",
		  "flags": {}
		}
	],
	"languages": [
		{
			"lang": "en",
			"name": "English",
			"path": "lang/en.json"
		}
	],
	"relationships": {
		"requires": [
			{
				"id": "routinglib",
				"type": "module",
				"manifest": "https://raw.githubusercontent.com/manuelVo/foundryvtt-routinglib/master/module.json",
				"compatibility": {}
			}
		]
	},
	"packs": [
		{
			"name": "ftl-macros",
			"label": "FTL-Macros",
			"path": "packs/ftl-macros",
			"type": "Macro",
			"ownership": {
				"PLAYER": "OBSERVER",
				"ASSISTANT": "OWNER"
			}
		}
	]
}